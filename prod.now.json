{
  "name": "web-clipper-extensions",
  "alias": ["extensions.clipper.website"],
  "builds": [{ "src": "extensions/**/*", "use": "@now/static" }],
  "routes": [
    {
      "src": "/extensions/(.*)",
      "dest": "/extensions/$1.json"
    },
    {
      "src": "/.*",
      "status": 404
    }
  ]
}
